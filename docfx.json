{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Dnn.Platform/DNN Platform/Library/DotNetNuke.Library.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "Dnn.Platform/DNN Platform/DotNetNuke.Log4net/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "sitemap": {
      "baseUrl": "https://dnndocs.com/"
    },
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["content/**.md", "content/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appLogoPath": "images/dnn_docs_logo-preview.png",
      "_gitContribute": {
        "repo": "https://github.com/DNNCommunity/DNNDocs.git",
        "branch": "master"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "templates/dnn-docs"],
    "postProcessors": ["TopContributorsProcessor"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false 
  }
}